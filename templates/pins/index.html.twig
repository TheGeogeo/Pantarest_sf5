{% extends 'base.html.twig' %}

{% block title "All best pins!" %}

{% block body %}
	<h1>{{pluralize(pins|length, 'Pin') }}</h1>
    {% for pin in pins %}
        <article>   
        <h2>{{pin.title}}</h2>
        <p>{{pin.description}}</p>
        </article>
    {% else %}
        
    {% endfor %}
{% endblock %}
